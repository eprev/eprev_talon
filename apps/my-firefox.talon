app: firefox
-
(pick | inspect) element: key(cmd-alt-c)
go to console: key(cmd-alt-k)
go to network: key(cmd-alt-e)
go to debugger: key(cmd-alt-z)
go to profiler: key(shift-f5)
go to storage: key(shift-f9)
responsive [design] mode: key(cmd-alt-m)
toggle dock: key(cmd-shift-d)
resume execution: key(f8)
step over: key(f10)
step into: key(f11)
step out: key(shift-f11)

